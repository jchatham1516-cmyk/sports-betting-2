        "import numpy as np\n",
        "\n",
        "def predict_game(model, home, away):\n",
        "    row = np.array([[\n",
        "        1,\n",
        "        home[\"ORtg\"] - away[\"ORtg\"],\n",
        "        away[\"DRtg\"] - home[\"DRtg\"],\n",
        "        home[\"eFG\"] - away[\"eFG\"],\n",
        "        away[\"TOV\"] - home[\"TOV\"],\n",
        "        home[\"AST\"] - away[\"AST\"],\n",
        "        home[\"ORB\"] - away[\"ORB\"],\n",
        "        home[\"DRB\"] - away[\"DRB\"],\n",
        "        home[\"FTAr\"] - away[\"FTAr\"],\n",
        "    ]])\n",
        "    return model.predict_proba(row)[0,1]\n",
        "\n"
      ],
