from nba_api.stats.endpoints import scoreboardv2
import pandas as pd
import os

# assuming you already have team_stats_df from fetch_team_advanced_stats()
TEAM_ID_TO_NAME = dict(zip(team_stats_df["TEAM_ID"], team_stats_df["TEAM_NAME"]))

def get_games_for_date(game_date_str):
    """
    game_date_str format: '2024-12-03'
    Returns a list of dicts with game_id, home_name, away_name.
    """
    sb = scoreboardv2.ScoreboardV2(
        game_date=game_date_str,
        league_id="00"
    )
    games_df = sb.game_header.get_data_frame()

    games = []
    for _, row in games_df.iterrows():
        home_name = TEAM_ID_TO_NAME[row["HOME_TEAM_ID"]]
        away_name = TEAM_ID_TO_NAME[row["VISITOR_TEAM_ID"]]
        games.append({
            "game_id": row["GAME_ID"],
            "home": home_name,
            "away": away_name,
        })
    return games


def run_daily_card(game_date_str, vegas_spreads=None):
    """
    vegas_spreads: optional dict keyed by GAME_ID -> vegas_home_spread
                   if you don't have it, we just use 0.0 as placeholder.
    """
    if vegas_spreads is None:
        vegas_spreads = {}

    os.makedirs("predictions", exist_ok=True)

    games = get_games_for_date(game_date_str)
    results = []

    for g in games:
        game_id = g["game_id"]
        vegas_home_spread = vegas_spreads.get(game_id, 0.0)  # fallback to 0

