        "# How many \"points\" each type of missing player is worth\n",
        "INJURY_WEIGHTS = {\n",
        "    \"star\": 3.0,      # main offensive/defensive engine\n",
        "    \"starter\": 1.5,   # normal starter\n",
        "    \"rotation\": 1.0,  # 6th8th man\n",
        "    \"bench\": 0.5,     # deep bench\n",
        "}\n"
      ],
