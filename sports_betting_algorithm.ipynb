        "\n",
        "def predict_spread(\n",
        "    home_team_name,\n",
        "    away_team_name,\n",
        "    vegas_home_spread,\n",
        "    home_injuries,\n",
        "    away_injuries,\n",
        "    home_team_stats,\n",
        "    away_team_stats,\n",
        "):\n",
        "    \"\"\"\n",
        "    Returns:\n",
        "      - model_spread (predicted point differential, home minus away)\n",
        "      - edge (model_spread  vegas_spread)\n",
        "      - whether it's a bet or fade\n",
        "    \"\"\"\n",
        "\n",
        "    # 1. Base team ratings (example structure)\n",
        "    home_off = home_team_stats[\"off_rating\"]\n",
        "    home_def = home_team_stats[\"def_rating\"]\n",
        "    away_off = away_team_stats[\"off_rating\"]\n",
        "    away_def = away_team_stats[\"def_rating\"]\n",
        "\n",
        "    # 2. Expected point differential from ratings\n",
        "    base_diff = (home_off - away_def) - (away_off - home_def)\n",
        "\n",
        "    # 3. Injury adjustments\n",
        "    # your previous code used +/- values from a dictionary\n",
        "    injury_adj = home_injuries - away_injuries\n",
        "\n",
        "    # 4. Home-court advantage\n",
        "    HCA = 2.7  # adjust if you want\n",
        "\n",
        "    model_spread = base_diff + injury_adj + HCA\n",
        "\n",
        "    # 5. Compare to Vegas line (home spread)\n",
        "    edge = model_spread - vegas_home_spread\n",
        "\n",
        "    if edge > 1:\n",
        "        verdict = f\"BET {home_team_name}\"\n",
        "    elif edge < -1:\n",
        "        verdict = f\"BET {away_team_name}\"\n",
        "    else:\n",
        "        verdict = \"NO BET / SMALL EDGE\"\n",
        "\n",
        "    return {\n",
        "        \"matchup\": f\"{home_team_name} (HOME) vs {away_team_name} (AWAY)\",\n",
        "        \"model_spread\": round(model_spread, 2),\n",
        "        \"vegas_spread\": vegas_home_spread,\n",
        "        \"edge\": round(edge, 2),\n",
        "        \"verdict\": verdict\n",
        "    }"
      ],
