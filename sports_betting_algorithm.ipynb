        "INJURY_ROLE_WEIGHTS = {\n",
        "    \"star\": 3.0,\n",
        "    \"starter\": 1.5,\n",
        "    \"rotation\": 1.0,\n",
        "    \"bench\": 0.5,\n",
        "}\n",
        "\n",
        "INJURY_STATUS_MULTIPLIER = {\n",
        "    \"out\": 1.0,\n",
        "    \"doubtful\": 0.75,\n",
        "    \"questionable\": 0.5,\n",
        "    \"probable\": 0.25,\n",
        "}\n"
      ],
